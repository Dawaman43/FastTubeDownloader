{
  "manifest_version": 3,
  "name": "FastTube Downloader",
  "version": "2.0",
  "description": "Download YouTube videos fast with IDM-style interface",
  "key": "",
  "permissions": [
    "nativeMessaging",
    "downloads",
    "activeTab",
    "scripting",
    "storage",
    "alarms",
    "notifications"
  ],
  "host_permissions": [
    "https://www.youtube.com/*",
    "http://www.youtube.com/*",
    "https://m.youtube.com/*",
    "https://*.youtube.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "http://www.youtube.com/*",
        "https://m.youtube.com/*",
        "https://*.youtube.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle",
      "all_frames": true
    }
  ]
}
